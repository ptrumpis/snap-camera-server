{
    "defaultEnv": "local",
    "local": {
        "driver": "mysql",
        "user": {
            "ENV": "DB_USER"
        },
        "password": {
            "ENV": "DB_PASS"
        },
        "database": {
            "ENV": "DB_NAME"
        },
        "host": {
            "ENV": "DB_HOST"
        },
        "port": {
            "ENV": "DB_PORT"
        },
        "multipleStatements": true
    },
    "sql-file": true
}